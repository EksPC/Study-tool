<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="840.0" prefWidth="70.0" style="-fx-background-color: rgb(99, 95, 89);" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="settingsButton" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@setting.png" />
         </image>
      </ImageView>
      <ImageView fx:id="focusButton" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@open-book.png" />
         </image>
         <VBox.margin>
            <Insets top="60.0" />
         </VBox.margin>
      </ImageView>
      <ImageView fx:id="todoButton" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
         <image>
            <Image url="@to-do-list.png" />
         </image>
      </ImageView>
      <ImageView fx:id="calendarButton" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
         <image>
            <Image url="@calendar.png" />
         </image>
      </ImageView>
   </children>
   <padding>
      <Insets left="10.0" top="20.0" />
   </padding>
</VBox>
